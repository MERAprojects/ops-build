- 		  : FIELD_SET(0, DE_CONTROL, DIRECTION, LEFT_TO_RIGHT)) |
- 		  FIELD_SET(0, DE_CONTROL, STATUS, START);
- 	write_dpr(accel, DE_CONTROL, de_ctrl); /* dpr0c */
