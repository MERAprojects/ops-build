- void hw_sm750_initAccel(struct sm750_dev *sm750_dev)
- {
- 	u32 reg;
- 
- 	enable2DEngine(1);
- 
- 	if (getChipType() == SM750LE) {
- 		reg = PEEK32(DE_STATE1);
- 		reg = FIELD_SET(reg, DE_STATE1, DE_ABORT, ON);
- 		POKE32(DE_STATE1, reg);
