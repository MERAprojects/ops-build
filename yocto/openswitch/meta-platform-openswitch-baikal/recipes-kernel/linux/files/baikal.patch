+ 	/* Don't consider an S-TAG (0x88A8) packet as a VLAN packet */
+ 	XGMAC_IOWRITE_BITS(pdata, MAC_VLANTR, ESVL, 0);
+ 
+ 	/* Enable VLAN tag stripping */
+ 	XGMAC_IOWRITE_BITS(pdata, MAC_VLANTR, EVLS, 0x3);
