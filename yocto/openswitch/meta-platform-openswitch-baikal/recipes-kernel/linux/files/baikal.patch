+    if (mips_cm_present()) {
+       unsigned long cm_error = read_gcr_error_cause();
+       unsigned long cm_addr = read_gcr_error_addr();
+       unsigned long cm_other = read_gcr_error_mult();
+       unsigned long cause, ocause;
+       char buf[256];
