+ static int xgbe_set_rss_lookup_table(struct xgbe_prv_data *pdata,
+ 				     const u32 *table)
+ {
+ 	unsigned int i;
+ DBGPR("-->%s\n", __func__);
+ 	for (i = 0; i < ARRAY_SIZE(pdata->rss_table); i++)
+ 		XGMAC_SET_BITS(pdata->rss_table[i], MAC_RSSDR, DMCH, table[i]);
+ 
+ 	return xgbe_write_rss_lookup_table(pdata);
+ }
