+ {
+ 	struct resource res;
+ 	struct device_node *np;
+ 
+ 	np = of_find_compatible_node(NULL, NULL, node);
+ 	if (!np)
+ 		panic("Failed to find %s node", node);
+ 
+ 	if (of_address_to_resource(np, 0, &res))
+ 		panic("Failed to get resource for %s", node);
+ 
+ 	if ((request_mem_region(res.start,
+ 				resource_size(&res),
+ 				res.name) < 0))
