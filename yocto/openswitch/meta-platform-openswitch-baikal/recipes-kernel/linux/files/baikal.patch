-  *          0   - Success
-  *         -1   - Fail
-  */
- int sm750_hw_i2c_write_reg(
- 	unsigned char addr,
- 	unsigned char reg,
- 	unsigned char data
- )
- {
- 	unsigned char value[2];
- 
- 	value[0] = reg;
- 	value[1] = data;
- 	if (hw_i2c_write_data(addr, 2, value) == 2)
- 		return 0;
- 
- 	return (-1);
- }
