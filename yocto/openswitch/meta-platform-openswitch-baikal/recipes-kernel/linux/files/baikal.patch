- 		  FIELD_SET(0, DE_DESTINATION, WRAP, DISABLE) |
- 		  FIELD_VALUE(0, DE_DESTINATION, X,    dx)  |
- 		  FIELD_VALUE(0, DE_DESTINATION, Y,    dy)); /* dpr04 */
