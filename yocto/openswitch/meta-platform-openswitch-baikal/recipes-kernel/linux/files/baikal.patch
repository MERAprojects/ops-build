+ 	int cdb[] = {ARRAY_SIZE(lynx750_ext), 0, VESA_MODEDB_SIZE};
+ 	static const char *mdb_desc[] = {
+ 		"driver prepared modes",
+ 		"kernel prepared default modedb",
+ 		"kernel HELPERS prepared vesa_modes",
+ 	};
+ 
+ 	static const char *fixId[2] = {
+ 		"sm750_fb1", "sm750_fb2",
