+ 	reg |= ((fmt<<DE_STRETCH_FORMAT_PIXEL_FORMAT_SHIFT)&DE_STRETCH_FORMAT_PIXEL_FORMAT_MASK);
+ 	write_dpr(accel, DE_STRETCH_FORMAT, reg);
