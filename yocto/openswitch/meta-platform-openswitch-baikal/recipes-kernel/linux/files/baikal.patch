+ 	if (pdata->hw_feat.hash_table_size) {
+ 		XGMAC_IOWRITE_BITS(pdata, MAC_PFR, HPF, 1);
+ 		XGMAC_IOWRITE_BITS(pdata, MAC_PFR, HUC, 1);
+ 		XGMAC_IOWRITE_BITS(pdata, MAC_PFR, HMC, 1);
