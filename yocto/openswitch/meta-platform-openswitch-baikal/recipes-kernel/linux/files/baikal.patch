- 				PANEL_FB_ADDRESS, ADDRESS, total));
- 	} else {
- 		POKE32(CRT_FB_ADDRESS,
- 			FIELD_VALUE(PEEK32(CRT_FB_ADDRESS),
- 				CRT_FB_ADDRESS, ADDRESS, total));
- 	}
- 	return 0;
- }
