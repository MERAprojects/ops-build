+ 	XGMAC_SET_BITS(arcache, DMA_AXIARCR, DRC, pdata->arcache);
+ 	XGMAC_SET_BITS(arcache, DMA_AXIARCR, DRD, pdata->axdomain);
+ 	XGMAC_SET_BITS(arcache, DMA_AXIARCR, TEC, pdata->arcache);
